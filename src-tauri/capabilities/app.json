{
  "identifier": "app",
  "description": "App permissions for dialog and filesystem",
  "platforms": ["windows", "linux", "macOS"],
  "windows": ["main"],
  "permissions": [
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-write-file",
    "fs:allow-copy-file",
    "fs:allow-exists"
  ]
}


